{
	"name": "training-tracker",
	"private": true,
	"version": "2.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "vue-tsc --noEmit && vite build",
		"preview": "vite preview",
		"api": "nodemon --ignore node_modules/ --ignore api/sessions.json --watch api/ --exec ts-node-esm api/app.ts",
		"build:server": "npx tsc api/app.ts --outDir server --target esnext --module nodenext"
	},
	"dependencies": {
		"compression": "^1.7.4",
		"cors": "^2.8.5",
		"cron": "^2.0.0",
		"dotenv": "^16.0.1",
		"express": "^4.18.1",
		"googleapis": "^105.0.0",
		"helmet": "^5.1.0",
		"lowdb": "^3.0.0",
		"node-fetch": "^3.2.6",
		"vue": "^3.2.25"
	},
	"devDependencies": {
		"@types/compression": "^1.7.2",
		"@types/cors": "^2.8.12",
		"@types/cron": "^2.0.0",
		"@types/express": "^4.17.13",
		"@types/node": "^17.0.43",
		"@vitejs/plugin-vue": "^2.3.3",
		"autoprefixer": "^10.4.7",
		"nodemon": "^2.0.18",
		"sass": "^1.52.3",
		"typescript": "^4.7.4",
		"unplugin-vue-components": "^0.19.6",
		"vite": "^2.9.9",
		"vite-plugin-html": "^3.2.0",
		"vue-tsc": "^0.34.7"
	}
